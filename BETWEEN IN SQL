-- BETWEEN TO RETRIEVE VALUE WITHIN A SPECIFIED RANGE
--BETWEEN and AND KEYWORDS WE USE 
--DATA TYPES :NUMBERS,STRING,DATE
--NOT BETWEEN ALSO USED 

--BETWEEN IN SQL 

SELECT * FROM EMPLOYEE WHERE SALARY BETWEEN 40000 AND 50000 ;

--TWO CONDITIONS

SELECT * FROM EMPLOYEE WHERE SALARY BETWEEN 40000 AND 50000 AND DEPARTMENT ='HR';

-- NOT BETWEEN 

SELECT * FROM EMPLOYEE WHERE SALARY NOT BETWEEN 620000 AND 68000;

-- USING STRING 

SELECT * FROM EMPLOYEE WHERE CITY BETWEEN 'CHENNAI' AND 'RANCHI';

-- USING ORDER BY 

SELECT * FROM EMPLOYEE WHERE CITY BETWEEN 'AHMEDABAD' AND 'RANCHI';


